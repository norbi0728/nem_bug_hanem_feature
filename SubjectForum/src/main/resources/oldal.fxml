                    <MenuItem mnemonicParsing="false" text="Bezárás" onAction="#exitProgram"/>
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Szerkesztés" />
                <Menu mnemonicParsing="false" text="Súgó">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Felhasználói kézikönyv" onAction="#loadUserManual" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
