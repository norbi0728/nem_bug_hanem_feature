                                 <children>
                                    <Label fx:id="subjectName" text="Label">
                                       <VBox.margin>
                                          <Insets left="10.0" />
                                       </VBox.margin>
                                       <font>
                                          <Font size="18.0" />
                                       </font>
                                       <HBox.margin>
                                          <Insets left="10.0" top="3.0" />
                                       </HBox.margin>
                                    </Label>
                                    <Button fx:id="updateButton" mnemonicParsing="false" onMouseClicked="#update" text="Frissítés">
